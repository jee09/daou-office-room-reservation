{
  "manifest_version": 3,
  "name": "다우오피스 회의실 예약",
  "version": "1.0",
  "description": "회의실 반복 예약 자동화",
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "popup.html"
  }
}
